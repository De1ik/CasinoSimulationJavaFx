<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="newGameVoting" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mycasinofx.controllers.voting.votingProcces.NewGameVotingController">
   <children>
      <Button layoutX="87.0" layoutY="334.0" mnemonicParsing="false" onAction="#goVoting" text="Back" />
