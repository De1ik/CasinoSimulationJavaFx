      <Button layoutX="274.0" layoutY="322.0" mnemonicParsing="false" onAction="#vote" text="Vote" />
      <RadioButton fx:id="radioBackarat" layoutX="107.0" layoutY="148.0" mnemonicParsing="false" text="Backarat" />
      <RadioButton fx:id="radioPoker" layoutX="107.0" layoutY="174.0" mnemonicParsing="false" text="Poker" />
      <RadioButton fx:id="radioDurak" layoutX="107.0" layoutY="200.0" mnemonicParsing="false" text="Durak" />
      <Label fx:id="warningLabel" layoutX="102.0" layoutY="304.0" text="Warnings" />
      <TextField fx:id="otherTextField" layoutX="107.0" layoutY="253.0" promptText="Enter your variant" />
      <RadioButton fx:id="radioOther" layoutX="107.0" layoutY="225.0" mnemonicParsing="false" text="Others" />
   </children>
</AnchorPane>
